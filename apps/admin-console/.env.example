NEXT_PUBLIC_API_BASE_URL=http://127.0.0.1:3000
NEXT_PUBLIC_SHARED_AUTH_CODE=admin
NEXT_PUBLIC_SHARED_USERNAME=admin
NEXT_PUBLIC_SHARED_PASSWORD=admin123

# Server-side admin proxy credentials (do NOT use NEXT_PUBLIC prefix).
ADMIN_PROXY_ROLE=admin
ADMIN_PROXY_AUTH_CODE=admin
ADMIN_PROXY_USERNAME=admin
ADMIN_PROXY_PASSWORD=admin123

# Local dev only default:
# - when NEXT_PUBLIC_API_BASE_URL is localhost/127.0.0.1, default admin secret is allowed.
# Non-local targets/shared-staging-prod MUST configure a non-default high-entropy value.
ADMIN_PROXY_SECRET=replace_with_high_entropy_secret
