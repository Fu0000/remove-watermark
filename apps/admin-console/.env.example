NEXT_PUBLIC_API_BASE_URL=http://127.0.0.1:3000
NEXT_PUBLIC_SHARED_AUTH_CODE=admin
NEXT_PUBLIC_SHARED_USERNAME=admin
NEXT_PUBLIC_SHARED_PASSWORD=admin123
NEXT_PUBLIC_ADMIN_ROLE=admin

